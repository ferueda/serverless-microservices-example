{
  "name": "functions",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start:signUp": "functions-framework --target=signUp",
    "dev:signUp": "concurrently \"tsc -w\" \"nodemon --watch ./dist/ --exec npm run start:signUp\"",
    "deploy:signUp": "gcloud functions deploy signUp --runtime nodejs12 --trigger-http --allow-unauthenticated",
    "start:deleteUserFromDb": "functions-framework --target=deleteUserFromDb --port 9000 --signature-type=event ",
    "dev:deleteUserFromDb": "concurrently \"tsc -w\" \"nodemon --watch ./dist/ --exec npm run start:deleteUserFromDb\"",
    "deploy:deleteUserFromDb": "gcloud functions deploy deleteUserFromDb --runtime nodejs12 --trigger-event providers/firebase.auth/eventTypes/user.delete"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@google-cloud/functions-framework": "^1.7.1",
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.22",
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.7",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@google-cloud/firestore": "^4.8.1",
    "express": "^4.17.1",
    "firebase": "^8.2.4"
  }
}
